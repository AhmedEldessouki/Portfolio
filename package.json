{
  "name": "portfolio",
  "version": "0.2.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "axios": "^0.20.0",
    "cloudinary-react": "^1.1.0",
    "firebase": "^7.20.0",
    "prop-types": "^15.7.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-dropzone": "^11.2.0",
    "react-icons": "^3.2.2",
    "react-redux": "^5.1.1",
    "react-redux-firebase": "^2.2.5",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-toastify": "^6.0.8",
    "redux": "^4.0.1",
    "redux-firestore": "^0.13.0",
    "redux-thunk": "^2.3.0",
    "serve": "^11.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@cypress/instrument-cra": "^1.3.2",
    "@testing-library/cypress": "^7.0.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/react-hooks": "^3.4.1",
    "@testing-library/user-event": "^12.1.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "cypress": "^5.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-kentcdodds": "^16.0.1",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-jest-dom": "^3.2.3",
    "husky": "^4.3.0",
    "is-ci": "^2.0.0",
    "jest": "24.9.0",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "jest-in-case": "^1.0.2",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.11.4"
  },
  "engines": {
    "node": ">10.19.0"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "start": "npm run build && serve -s build",
    "setup": "npm install && npm run validate",
    "validate": "npm run build",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|yml|yaml|html)\"",
    "lint": "eslint .",
    "cy": "is-ci \"test:e2e\" \"cy:open\"",
    "cy:open": " cypress open",
    "cy:run": " cypress run",
    "test:e2e": "start-server-and-test start http://localhost:3000 cy:run",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
