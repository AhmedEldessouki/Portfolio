{
  "name": "portfolio",
  "version": "2.0.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "axios": "^0.21.1",
    "cloudinary-react": "^1.6.8",
    "debounce-fn": "^4.0.0",
    "firebase": "^8.2.1",
    "is-ci": "^2.0.0",
    "match-sorter": "^6.0.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.2.4",
    "react-error-boundary": "^3.1.0",
    "react-icons": "^4.1.0",
    "react-query": "^3.5.6",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-toastify": "^6.2.0",
    "react-transition-group": "^4.4.1",
    "serve": "^11.3.2",
    "workbox-core": "^6.0.2",
    "workbox-expiration": "^6.0.2",
    "workbox-precaching": "^6.0.2",
    "workbox-routing": "^6.0.2",
    "workbox-strategies": "^6.0.2"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.7.0",
    "@testing-library/user-event": "^12.6.0",
    "cypress": "^6.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.16.0",
    "eslint-config-kentcdodds": "^17.3.0",
    "eslint-plugin-cypress": "^2.11.2",
    "faker": "^5.1.0",
    "husky": "^4.3.6",
    "jest": "26.6.0",
    "lint-staged": "^10.5.3",
    "msw": "^0.24.4",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "start-server-and-test": "^1.11.7",
    "workbox-build": "^6.0.2"
  },
  "engines": {
    "node": ">10.19.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "start": "set PORT=5000 && react-scripts start",
    "dev": "react-scripts start",
    "setup": "npm install && npm run validate",
    "validate": "npm run build && npm run test:coverage",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|yml|yaml|html)\"",
    "lint": "eslint . --cache-location node_modules/.cache/eslint",
    "test": "jest --watch=false --findRelatedTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --watch=false --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --watch --runInBand",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test start http://localhost:5000 cy:run"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageReporters": [
      "text"
    ]
  }
}
